		<servlet-class>br.com.nascisoft.apoioterritoriols.cadastro.server.CadastroServiceImpl</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>CadastroServlet</servlet-name>
		<url-pattern>/cadastro/CadastroService</url-pattern>
	</servlet-mapping>

